{
  "prettier.tabWidth": 4,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte"
  ],
  "coc.preferences.formatOnSaveFiletypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "svelte",
    "yaml"
  ],
  "omnisharp.version":"v1.37.3",
  "omnisharp.trace.server": "information",
  "omnisharp.debug.server": true,
  "tsserver.formatOnType": false,
