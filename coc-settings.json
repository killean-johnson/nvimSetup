  "snippets.ultisnips.directories": [ "$HOME/.config/nvim/plugged" ],
  "snippets.extends": {
    "typescriptreact": [ "javascript" ]
  },
  "tailwindCSS.htmlLanguages": [
    "blade",
    "edge",
    "eelixir",
